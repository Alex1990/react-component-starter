{
  "name": "dice-roll",
  "version": "0.1.0",
  "description": "A starter based on webpack ecosystem to develop react components comfortably",
  "main": "./lib/index.js",
  "module": "./src/index.js",
  "author": "Alex Chao",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:Alex1990/react-component-starter.git"
  },
  "scripts": {
    "start": "NODE_ENV=development node ./scripts/server",
    "compile": "rimraf lib && babel -d lib src",
    "build": "rimraf dist &&  NODE_ENV=production webpack",
    "bundle-analyzer": "NODE_ENV=production webpack --json > stats.json",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:cover": "npm run test -- --coverage",
    "test:CI": "npm run test",
    "test:cover:CI": "npm run test:cover && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint src"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "glob": "^7.1.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "progress-bar-webpack-plugin": "^1.9.3",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.5.8"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 9"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss|styl)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
